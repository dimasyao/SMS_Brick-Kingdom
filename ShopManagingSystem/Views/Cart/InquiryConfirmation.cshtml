@model SMS_Models.OrderHeader

<div class="text-center">

    @if (Model != null)
    {
        <h1 class="text-info"> Order created </h1>
        <p> Order #@Model.Id has been created successfully!</p>
    }
    else
    {
        <h1 class="text-info">Inquiry Confirmation</h1>
        <p>We have received your inquiry and someone from our team will contact you!</p>
    }
</div>